{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "./client/build",
            "use": "@vercel/static"
        }
    ],

    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "index.ts"
        }
    ]
}
